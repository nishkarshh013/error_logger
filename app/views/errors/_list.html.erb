<% error_groups.each do |group| %>
  <%= turbo_frame_tag dom_id(group) do %>
    <div class="error-group-card">
      <h3><%= group.error_class %></h3>
      <p><%= truncate(group.message, length: 100) %></p>
      <p><%= group.error_occurrences.count %> occurrences</p>
      <%#= link_to "View", app_error_group_path(@app, group), class: "btn" %>
    </div>
  <% end %>
<% end %>
